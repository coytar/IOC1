<Window x:Class="IOC1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IOC1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="312,178,0,0" TextWrapping="Wrap" Text="{Binding Name}" VerticalAlignment="Top" Height="36" Width="152"/>
        <Button Content="Button" Command="{Binding Button1PressCommand}" HorizontalAlignment="Left" Margin="312,96,0,0" VerticalAlignment="Top" Width="152" Height="28"/>

    </Grid>
</Window>
